    static Node oddEvenList(Node head) {
        Node odd=new Node(0);
        Node even=new Node(0);
        Node oddTemp=odd;
        Node evenTemp=even;
        Node temp=head;
        while(temp!=null){
            oddTemp.next=temp;
            temp=temp.next;
            oddTemp=oddTemp.next;


            evenTemp.next=temp;
            if(temp==null)break;
            temp=temp.next;
            evenTemp=evenTemp.next;

        }
        odd=odd.next;
        even=even.next;
        oddTemp.next=even;

        return odd;
    }
