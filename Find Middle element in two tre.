package LinkedListbassic;

public class LinkedListPrint {
    static class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
        }
    }
    static int middle(Node head) {
        Node fast = head;
        int size = 0;
        while (fast.next != null) {
            size++;
            fast = fast.next;
        }
        int m=size/2;
        Node slow=head;
       for(int i=1;i<=m;i++) {
           slow = slow.next;
       }
       return slow.data;

    }

        static void printList(Node node) {
            while (node != null) {
                System.out.print(node.data + " ");
                node = node.next;
            }
            System.out.println();
        }

        public static void main(String[] args) {
            Node a = new Node(1);
            Node b = new Node(2);
            Node c = new Node(7);
            Node d = new Node(8);
            Node e = new Node(9);
            Node f = new Node(10);
            Node g = new Node(11);

            a.next = b;
            b.next = c;
            c.next = d;
            d.next = e;
            e.next = f;
            f.next = g;

            printList(a);
            int a1=middle(a);
           System.out.println(a1);
        }
    }
