package LinkedListbassic;

public class LinkedListPrint {
    static class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
        }
    }
    static boolean cycle(Node head) {
        Node slow = head;
        Node fast = head;
        while(fast.next!=null){
            slow = slow.next;
            fast = fast.next.next;
            if(slow==fast){
                return true;
            }
        }
        return false;
    }
    

        public static void main(String[] args) {
            Node a = new Node(1);
            Node b = new Node(2);
            Node c = new Node(7);
            Node d = new Node(8);
            Node e = new Node(9);
            Node f = new Node(10);
            Node g = new Node(11);

            a.next = b;
            b.next = c;
            c.next = d;
            d.next = e;
            e.next = f;
            f.next = g;
            g.next = d;

           boolean flag = cycle(a);
           System.out.println(flag);

        }
    }
