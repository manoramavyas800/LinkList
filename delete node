package LinkedListbassic;

public class LinkedListPrint {
    static class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
        }
    }
     static void deleteNode(Node node){
        node.data = node.next.data;
        node.next = node.next.next;
    }
   static void printList(Node node){
        while(node != null){
            System.out.print(node.data + " ");
            node = node.next;
        }
        System.out.println();
    }
    public static void main(String[] args) {
        Node a = new Node(1);
        Node b = new Node(2);
        Node c = new Node(3);
        Node d = new Node(4);
        Node e = new Node(5);
        a.next = b;
        b.next = c;
        c.next = d;
        d.next = e;

        printList(a);
        deleteNode(c);
        printList(a);

    }
}
